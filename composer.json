{
    "name": "laravel/laravel",
    "type": "project",
    "description": "KonturCMS based on Laravel Framework.",
    "keywords": [
        "framework",
        "laravel",
        "KonturCMS"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ezyang/htmlpurifier": "^4.16",
        "google/recaptcha": "^1.3",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.0",
        "intervention/image": "^3.2",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "tightenco/ziggy": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Kontur\\Dashboard\\": "packages/kontur/dashboard/src",
            "Galtsevt\\LaravelStorage\\": "packages/galtsevt/laravel-storage/src",
            "Galtsevt\\LaravelSeo\\": "packages/galtsevt/laravel-seo/src",
            "Galtsevt\\AppConf\\": "packages/galtsevt/appconf/src"
        },
        "files": [
            "packages/galtsevt/appconf/src/app/helpers/helpers.php",
            "modules/InfoBlocks/App/Functions/iblock.php",
            "packages/galtsevt/laravel-seo/src/App/Functions/seo.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "installer-types": [
            "kontur-module"
        ],
        "installer-paths": {
            "modules/{$vendor}/{$name}/": [
                "type:kontur-module"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "composer/installers": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
